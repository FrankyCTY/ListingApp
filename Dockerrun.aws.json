{
  "AWSEBDockerrunVersion": 2,
  "containerDefinitions": [
    {
      "name": "client",
      "image": "frankypassword123/listingapp-client",
      "hostname": "client",
      "essential": false,
      "memory": 400
    },
    {
      "name": "server",
      "image": "frankypassword123/listingapp-server",
      "hostname": "api",
      "essential": false,
      "memory": 350
    },
    {
      "name": "nginx",
      "image": "frankypassword123/listingapp-nginx",
      "hostname": "nginx",
      "essential": true,
      "portMappings": [
        {
          "hostPort": 80,
          "containerPort": 80
        }
      ],
      "links": ["client", "server"],
      "memory": 200
    }
  ]
}
